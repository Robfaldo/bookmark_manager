<ul>
  <% @bookmarks.each do |bookmark| %>
  <div class ='row'>
    <div class = 'col-lg-1'><%= bookmark %></div>
  </div>
  <% end %>
  <%= @bookmark_to_add %>
</ul>

<%= styled_flash %>

<form action="/bookmarks" method="post">
  Enter a bookmark<br>
  <input type="text" name="bookmark">
  Enter the title<br>
  <input type="text" name="title">
  <input type="submit" value="submit">
</form>
